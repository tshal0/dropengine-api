{
  "name": "dropengine-api",
  "version": "1.0.2",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "tsc": "tsc",
    "tsc:debug": "tsc --showConfig --listFilesOnly",
    "build": "nest build",
    "build:l": "dotenv -e dropengine.local.env -- yarn build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dev:l": "dotenv -e .env.local -- nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:debug:l": "dotenv -e .env.local -- nest start --debug 0.0.0.0:9229 --watch",
    "sdl": "dotenv -e dropengine.local.env -- nest start --debug 0.0.0.0:9229 --watch",
    "sdd": "dotenv -e dropengine.staging.env -- nest start --debug 0.0.0.0:9229 --watch",
    "sdp": "dotenv -e dropengine.prod.env -- nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --maxWorkers=50% --force-exit --detectOpenHandles --config jest.config.ts",
    "test:watch": "jest --maxWorkers=50% --watch --detectOpenHandles --config jest.config.ts",
    "test:cov": "jest --maxWorkers=50% --config jest.config.ts --coverage --force-exit --detectOpenHandles",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --detectOpenHandles --maxWorkers=50%",
    "test:e2e": "yarn migrate & yarn jest --config ./jest-e2e.config.ts --detectOpenHandles --force-exit",
    "test:e2e:local": "dotenv -e dropengine.local.env -- yarn jest --config ./jest-e2e.config.ts --detectOpenHandles --force-exit",
    "test:e2e:docker": "docker-compose -f docker-compose.e2e.yml --env-file e2e.docker.env run --rm e2e",
    "test:postman": "newman run postman/drop-engine-api-tests.postman_collection.json -e postman/dev.postman_environment.json --reporters cli,junitfull --reporter-junitfull-export junit.postman.xml --insecure",
    "docker": "docker-compose --env-file .env up --build --remove-orphans",
    "docker:build": "docker build -t dropengine-api:1.0.1 --no-cache -f Dockerfile .",
    "dbd:drop": "dotenv -e .env -- yarn mikro-orm schema:drop",
    "dbd:create": "dotenv -e .env -- yarn mikro-orm migration:create",
    "dbd:up": "dotenv -e .env -- yarn mikro-orm migration:up",
    "dbl:drop": "dotenv -e .env.local -- yarn mikro-orm schema:drop",
    "dbl:create": "dotenv -e .env.local -- yarn mikro-orm migration:create",
    "dbl:up": "dotenv -e .env.local -- yarn mikro-orm migration:up",
    "dbl:down": "dotenv -e .env.local -- yarn mikro-orm migration:down",
    "migrate": "mikro-orm migration:up",
    "migrate:local": "dotenv -e dropengine.local.env -- yarn mikro-orm migration:up"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.8.0",
    "@mikro-orm/core": "^5.0.2",
    "@mikro-orm/migrations": "^5.0.2",
    "@mikro-orm/nestjs": "^4.3.1",
    "@mikro-orm/postgresql": "^5.0.2",
    "@nestjs/axios": "^0.0.5",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.0.0",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/mongoose": "^9.0.3",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.1.5",
    "@types/chalk": "^2.2.0",
    "applicationinsights": "^2.3.1",
    "auth0": "^2.37.0",
    "cache-manager": "^3.6.0",
    "chalk": "4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie": "^0.4.1",
    "crypto": "^1.0.1",
    "csvtojson": "^2.0.10",
    "dinero.js": "^1.9.1",
    "dotenv-cli": "^4.1.1",
    "https": "^1.0.0",
    "jwks-rsa": "^2.0.5",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "mongoose": "^6.2.10",
    "multer": "^1.4.4",
    "nest-winston": "^1.6.2",
    "node-mocks-http": "^1.11.0",
    "nonce": "^1.0.4",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "querystring": "^0.2.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "rxjs": "^7.2.0",
    "safe-json-stringify": "^1.2.0",
    "swagger-ui-express": "^4.3.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@jest/types": "^27.4.2",
    "@mikro-orm/cli": "^5.0.2",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/lodash": "^4.14.178",
    "@types/mongodb-memory-server": "^2.3.0",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/safe-json-stringify": "^1.1.2",
    "@types/supertest": "^2.0.11",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv-expand": "^8.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "jest-html-reporters": "^3.0.6",
    "jest-junit": "^13.0.0",
    "jest-module-name-mapper": "^0.1.5",
    "mongodb-memory-server": "^8.5.1",
    "newman": "^5.3.2",
    "newman-reporter-htmlextra": "^1.22.7",
    "newman-reporter-json": "^1.0.0",
    "newman-reporter-junitfull": "^1.1.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "run-script-webpack-plugin": "^0.0.11",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "webpack": "^5.67.0",
    "webpack-node-externals": "^3.0.0"
  },
  "_moduleAliases": {
    "@shared": "dist/shared",
    "@auth": "dist/auth",
    "@catalog": "dist/catalog",
    "@shopify": "dist/shopify",
    "@auth0": "dist/auth0",
    "@sales": "dist/sales"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./mikro-orm.config.ts",
      "./src/mikro-orm.config.ts"
    ]
  },
  "config": {
    "mongodbMemoryServer": {}
  }
}
